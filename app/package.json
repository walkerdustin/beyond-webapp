{
	"name": "beyond-webapp",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"prepare": "npm run smui-theme && npm run site-light",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme",
		"site-light": "sass --no-source-map -I src/theme -I node_modules -I ../smui-theme/node_modules -I ../../node_modules src/site.scss static/site.css"
	},
	"devDependencies": {
		"@material/elevation": "^13.0.0",
		"@material/typography": "^13.0.0",
		"@mdi/js": "^7.0.96",
		"@playwright/test": "^1.22.2",
		"@smui/button": "^6.1.4",
		"@smui/card": "^7.0.0-beta.0",
		"@smui/circular-progress": "^6.1.4",
		"@smui/common": "^6.1.4",
		"@smui/dialog": "^6.1.4",
		"@smui/icon-button": "^7.0.0-beta.0",
		"@smui/linear-progress": "^6.1.4",
		"@smui/paper": "^6.1.4",
		"@smui/ripple": "^7.0.0-beta.0",
		"@smui/snackbar": "^6.1.4",
		"@smui/tab": "^6.1.4",
		"@smui/tab-bar": "^6.1.4",
		"@smui/textfield": "^7.0.0-beta.0",
		"@smui/tooltip": "^6.1.4",
		"@smui/top-app-bar": "^6.1.4",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "^1.0.0-next.38",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.7",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.55.0",
		"smui-theme": "^6.2.0",
		"supabase-ui-svelte": "^0.1.1",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.5",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.0.0"
	},
	"type": "module",
	"dependencies": {
		"@supabase/supabase-js": "^2.0.4"
	}
}
